{
  "functions": {
    "server.js": {
      "memory": 3008,
      "maxDuration": 60,
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/((?!api/.*).*)",
      "destination": "/app.html"
    }
  ],
  "buildCommand": "npm install",
  "env": {
    "PLAYWRIGHT_BROWSERS_PATH": "0"
  }
}